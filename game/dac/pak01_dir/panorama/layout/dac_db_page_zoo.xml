	<styles>
		<include src="s2r://panorama/styles/dac_global.vcss_c" />
		<include src="s2r://panorama/styles/dac_db_page_zoo.vcss_c" />
        <include src="s2r://panorama/styles/dac_ui_zoo.vcss_c" />
