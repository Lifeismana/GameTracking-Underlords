<root>
	<styles>
		<include src="s2r://panorama/styles/gamestyles.vcss_c" />
		<include src="s2r://panorama/styles/dac_global.vcss_c" />
		<include src="s2r://panorama/styles/dac_ui_underlord_tree.vcss_c" />
	</styles>

  <DACUnderlordTree>
    <Image id="TalentPassive" class="PassiveTalent" scaling="stretch-to-cover-preserve-aspect"/>
    <Panel id="TalentOptions">
      <Panel id="TalentOptionRow4" class="TalentOptionRow">
        <Panel class="TalentOptionContainer Left">
          <Image id="OptionLeft" class="TalentOption" scaling="stretch-to-cover-preserve-aspect" onactivate="DACUnderlordTreeSelectTalent( 0, 0 )"/>
        </Panel>
        <Panel class="LevelSection">
          <Panel class="Connector"></Panel>
          <Panel class="LevelCircle">
            <Label class="LevelValue" text="4"/>
          </Panel>
        </Panel>
        <Panel class="TalentOptionContainer Right">
          <Image id="OptionRight" class="TalentOption" scaling="stretch-to-cover-preserve-aspect" onactivate="DACUnderlordTreeSelectTalent( 0, 1 )"/>
        </Panel>
      </Panel>
      <Panel id="TalentOptionRow6" class="TalentOptionRow">
        <Panel class="TalentOptionContainer Left">
          <Image id="OptionLeft" class="TalentOption" scaling="stretch-to-cover-preserve-aspect" onactivate="DACUnderlordTreeSelectTalent( 1, 0 )"/>
        </Panel>
        <Panel class="LevelSection">
          <Panel class="Connector"></Panel>
          <Panel class="LevelCircle">
            <Label class="LevelValue" text="6"/>
          </Panel>
        </Panel>
        <Panel class="TalentOptionContainer Right">
          <Image id="OptionRight" class="TalentOption" scaling="stretch-to-cover-preserve-aspect" onactivate="DACUnderlordTreeSelectTalent( 1, 1 )"/>
        </Panel>
      </Panel>
      <Panel id="TalentOptionRow8" class="TalentOptionRow">
        <Panel class="TalentOptionContainer Left">
          <Image id="OptionLeft" class="TalentOption" scaling="stretch-to-cover-preserve-aspect" onactivate="DACUnderlordTreeSelectTalent( 2, 0 )"/>
        </Panel>
        <Panel class="LevelSection">
          <Panel class="Connector"></Panel>
          <Panel class="LevelCircle">
            <Label class="LevelValue" text="8"/>
          </Panel>
        </Panel>
        <Panel class="TalentOptionContainer Right">
          <Image id="OptionRight" class="TalentOption" scaling="stretch-to-cover-preserve-aspect" onactivate="DACUnderlordTreeSelectTalent( 2, 1 )"/>
        </Panel>
      </Panel>
      <Panel id="TalentOptionRow10" class="TalentOptionRow">
        <Panel class="TalentOptionContainer Left">
          <Image id="OptionLeft" class="TalentOption" scaling="stretch-to-cover-preserve-aspect" onactivate="DACUnderlordTreeSelectTalent( 3, 0 )"/>
        </Panel>
        <Panel class="LevelSection">
          <Panel class="Connector"></Panel>
          <Panel class="LevelCircle">
            <Label class="LevelValue" text="10"/>
          </Panel>
        </Panel>
        <Panel class="TalentOptionContainer Right">
          <Image id="OptionRight" class="TalentOption" scaling="stretch-to-cover-preserve-aspect" onactivate="DACUnderlordTreeSelectTalent( 3, 1 )"/>
        </Panel>
      </Panel>
    </Panel>
  </DACUnderlordTree>
</root>
