<root>
	<styles>
		<include src="s2r://panorama/styles/gamestyles.vcss_c" />
		<include src="s2r://panorama/styles/dac_db_control_rank.vcss_c" />
	</styles>

	<DACControlRank>
		<DACRankIcon id="RankIconStandard" class="RankIcon" iconstyle="standalone" ranktype="standard"/>
		<DACRankIcon id="RankIconTurbo" class="RankIcon" iconstyle="standalone" ranktype="turbo"/>
		<Panel class="RankContainer">
			<Label id="StandardTitle" class="Title" html="true" text="#DAC_MatchTypeStandard" />
			<Label id="TurboTitle" class="Title" html="true" text="#DAC_MatchTypeTurbo" />
			<Label class="RankName HeadlineFont" html="true" text="#DAC_LevelUp_Rank_New" />
			<ProgressBar id="RankProgressBar" class="RankProgressBar"/>
			<Label class="RankScore" html="true" text="#DACRankPoints" />
		</Panel>
	</DACControlRank>
</root>
