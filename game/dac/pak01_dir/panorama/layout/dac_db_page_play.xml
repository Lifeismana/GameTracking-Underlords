              </Panel>

              <Panel id="BattlepassGCDuosBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(5,4)">
                <Label text="#DAC_BotDifficultyName5"/>
              </Panel>

              <Panel id="NoBotGCDuosBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(5,0)">
                <Label text="#DAC_BotDifficultyName4"/>
