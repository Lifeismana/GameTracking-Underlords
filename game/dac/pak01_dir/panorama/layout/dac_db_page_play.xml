              <Panel id="NoBotGCTurboBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(1,0)">
                <Label text="#DAC_BotDifficultyName4"/>
              </Panel>
            </Panel>

          </Panel>

          <!-- *************************Standard Options************************* -->
          <Panel id="StandardOptions" class="GameModeOptionsPanel">
            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="GameModeOptionsTitle HeadlineFont" text="#DAC_MatchTypeStandard"/>
              <!--<Label class="GameModeOptionsDesc" text="#DAC_MatchTypeDescStandard"/>-->
            </Panel>

            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="OptionHeader" text="#DAC_MatchOpponent"/>
              <Panel id="StandardMatchTypeButtonMultiplayer" class="GameModeOptionButton" onactivate="DACPlayStandardSetMatchType(0)">
                <Label text="#DAC_MatchTypeMultiplayer"/>
              </Panel>

              <Panel id="StandardMatchTypeButtonBots" class="GameModeOptionButton" onactivate="DACPlayStandardSetMatchType(3)">  <!--EDACMatchMode-->
