            <!-- <Panel class="GameModeOptionsDiv"/> -->

            <Panel class="GameModeOptionsSettingsContainer">
              <Panel id="StandardSubSettingMultiplayer" class="GameModeOptionsSettingsSubContainer">
                <Label class="OptionHeader" text="#DAC_MatchType"/>
                <Panel id="StandardMatchmakingTypeButtonCasual" class="GameModeOptionButton" onactivate="DACPlayStandardSetMatchmakingType(1)">  <!--EDACMatchMode-->
                  <Label text="#DAC_MatchTypeName0"/>
                </Panel>

                <Panel id="StandardMatchmakingTypeButtonRanked" class="GameModeOptionButton" onactivate="DACPlayStandardSetMatchmakingType(4)">  <!--EDACMatchMode-->
                  <Label text="#DAC_MatchTypeNameRanked"/>
                </Panel>

              </Panel>

              <Panel id="StandardSubSettingBots" class="GameModeOptionsSettingsSubContainer">
                <Label class="OptionHeader" text="#DAC_BotDifficulty_Desc"/>
                <Panel id="LocalStandardBotDifficultyButton" class="BotDifficultyDropdown" onactivate="DACPlayDisplayBotDifficulty(2)">  <!--EPlayBotDifficultyType-->
                  <Label text="{s:local_standard_bot_difficulty}"/>
                </Panel>
                <Panel id="GCStandardBotDifficultyButton" class="BotDifficultyDropdown" onactivate="DACPlayDisplayBotDifficulty(3)">  <!--EPlayBotDifficultyType-->
                  <Label text="{s:gc_standard_bot_difficulty}"/>
                </Panel>
              </Panel>

              <Panel id="StandardSubSettingResume" class="GameModeOptionsSettingsSubContainer">
                <Panel id="StandardResumeBotGameButton" class="GameModeOptionButton" onactivate="DACPlaySetSaveGameAction(false)">
                  <Label text="#DAC_Play_ResumeBots"/>
                </Panel>
                <Panel id="StandardDeleteBotGameButton" class="GameModeOptionButton" onactivate="DACPlaySetSaveGameAction(true)">
                  <Label text="#DAC_Home_DeleteSave"/>
                </Panel>

                <Panel class="ResumeInfoContainer">
                  <Label class="PlayPageLabel ResumeLabel" text="#DAC_ResumeInfo_Desc"/>
                  <Label class="PlayPageLabel ResumeLabel" text="#DAC_ResumeInfo_Timestamp"/>
                </Panel>
              </Panel>
            </Panel>
          </Panel>

          <!-- *************************Standard Bot Difficulty************************* -->
          <Panel id="StandardBotDifficulty" class="GameModeOptionsPanel">
            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="GameModeOptionsTitle HeadlineFont" text="#DAC_MatchTypeStandard"/>
            </Panel>
            <Label class="OptionHeader" text="#DAC_BotDifficulty_Desc"/>
