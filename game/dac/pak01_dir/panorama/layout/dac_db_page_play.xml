                  <Label text="#DAC_Home_DeleteSave"/>
                </Panel>

                <Panel class="ResumeInfoContainer">
                  <Label class="PlayPageLabel ResumeLabel" text="#DAC_ResumeInfo_Desc"/>
                  <Label class="PlayPageLabel ResumeLabel" text="#DAC_ResumeInfo_Timestamp"/>
                </Panel>
              </Panel>
            </Panel>


            <Panel class="GameModeOptionsSettingsContainer">
              <Panel id="DuosSubSettingTeammate" class="GameModeOptionsSettingsSubContainer">
              <Label class="OptionHeader" text="#DAC_MatchTeammate"/>
<!--
                <DropDown id="DuosTeammateDropdown" class="GameModeOptionsDropdown">
                  <Label id="Invite" text="#DAC_MatchTypeInviteAFriend" TeammateType="0"/>
                  <Label id="Bot" text="#DAC_MatchTypeBotTeammate" TeammateType="1"/>
                </DropDown>
-->
                <Panel id="DuosTeammateTypeButtonInvite" class="GameModeOptionButton" onactivate="DACPlayDuosSetTeammateType(0)">
                  <Label text="#DAC_MatchTypeInviteAFriend"/>
                </Panel>

                <Panel id="DuosTeammateTypeButtonBot" class="GameModeOptionButton" onactivate="DACPlayDuosSetTeammateType(1)">
                  <Label text="#DAC_MatchTypeBotTeammate"/>
                </Panel>

              </Panel>
            </Panel>
          </Panel>

          <!-- *************************Duos Bot Difficulty************************* -->
          <Panel id="DuosBotDifficulty" class="GameModeOptionsPanel">
            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="GameModeOptionsTitle HeadlineFont" text="#DAC_MatchTypeNameDuos"/>
            </Panel>

            <Panel id="LocalDuosBotDifficultyContainer" class="GameModeOptionsSettingsContainer">
              <Label class="OptionHeader" text="#DAC_BotDifficulty_Desc"/>
              <Panel id="EasyLocalDuosBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(4,0)">
