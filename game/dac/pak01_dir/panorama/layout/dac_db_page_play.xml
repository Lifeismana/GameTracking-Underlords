              <Panel id="NoBotGCStandardBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(3,0)">
                <Label text="#DAC_BotDifficultyName4"/>
              </Panel>
            </Panel>

          </Panel>

          <!-- *************************Duos Options************************* -->
          <Panel id="DuosOptions" class="GameModeOptionsPanel">
            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="GameModeOptionsTitle HeadlineFont" text="#DAC_MatchTypeNameDuos"/>
              <!-- <Label class="GameModeOptionsDesc" text="#DAC_MatchTypeDescDuosCasual"/> -->
            </Panel>

            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="OptionHeader" text="#DAC_MatchOpponent"/>
              <Panel id="DuosMatchTypeButtonMultiplayer" class="GameModeOptionButton" onactivate="DACPlayDuosSetMatchType(0)">
                <Label text="#DAC_MatchTypeMultiplayer"/>
              </Panel>

              <Panel id="DuosMatchTypeButtonBots" class="GameModeOptionButton" onactivate="DACPlayDuosSetMatchType(3)">  <!--EDACMatchMode-->
                <Label text="#DAC_MatchTypeBotMatch"/>
              </Panel>
            </Panel>

            <Panel class="GameModeOptionsSettingsContainer">
              <Panel id="DuosSubSettingMultiplayer" class="GameModeOptionsSettingsSubContainer">
                <Label class="OptionHeader" text="#DAC_MatchType"/>
                <Panel id="DuosMatchmakingTypeButtonCasual" class="GameModeOptionButton" onactivate="DACPlayDuosSetMatchmakingType(1)">  <!--EDACMatchMode-->
                  <Label text="#DAC_MatchTypeName0"/>
                </Panel>

                <Panel id="DuosMatchmakingTypeButtonRanked" class="GameModeOptionButton" onactivate="DACPlayDuosSetMatchmakingType(4)">  <!--EDACMatchMode-->
                  <Label text="#DAC_MatchTypeNameRanked"/>
                </Panel>

              </Panel>

              <Panel id="DuosSubSettingBots" class="GameModeOptionsSettingsSubContainer">
                <Label class="OptionHeader" text="#DAC_BotDifficulty_Desc"/>
                <Panel id="LocalDuosBotDifficultyButton" class="BotDifficultyDropdown" onactivate="DACPlayDisplayBotDifficulty(4)">  <!--EPlayBotDifficultyType-->
                  <Label text="{s:local_duos_bot_difficulty}"/>
                </Panel>
                <Panel id="GCDuosBotDifficultyButton" class="BotDifficultyDropdown" onactivate="DACPlayDisplayBotDifficulty(5)">  <!--EPlayBotDifficultyType-->
                  <Label text="{s:gc_duos_bot_difficulty}"/>
