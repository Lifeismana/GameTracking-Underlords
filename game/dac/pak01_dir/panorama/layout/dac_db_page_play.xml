      </Panel>
      
      <Panel id="Footer">
        
        <Panel id="UnderlordShadow"/>
        <Panel id="ViewChallenges" onactivate="DACPlayViewSeasonInfo()">
          <Label text="#DAC_BattlePassTitle"/>
        </Panel>
        
        <Panel id="PlayButtonContainer">
          <Panel id="TurboPlayButton" class="PlayButton" onactivate="DACPlayLaunchTurbo()">
            <Label id="TurboFindMatchLabel" class="FindMatchLabel HeadlineFont" text="#DAC_PreMatchDetails_FindMatch"/>
            <Label id="TurboBeginMatchLabel" class="BeginMatchLabel HeadlineFont" text="#DAC_PreMatchDetails_BeginMatch"/>
          </Panel>
          <Panel id="StandardPlayButton" class="PlayButton" onactivate="DACPlayLaunchStandard()">
            <Label id="StandardFindMatchLabel" class="FindMatchLabel HeadlineFont" text="#DAC_PreMatchDetails_FindMatch"/>
            <Label id="StandardBeginMatchLabel" class="BeginMatchLabel HeadlineFont" text="#DAC_PreMatchDetails_BeginMatch"/>
          </Panel>
          <Panel id="DuosPlayButton" class="PlayButton" onactivate="DACPlayLaunchDuos()">
            <Label id="DuosFindMatchLabel" class="FindMatchLabel HeadlineFont" text="#DAC_PreMatchDetails_FindMatch"/>
            <Label id="DuosBeginMatchLabel" class="BeginMatchLabel HeadlineFont" text="#DAC_PreMatchDetails_BeginMatch"/>
            <Label id="DuosInviteFriendLabel" class="InviteFriendLabel HeadlineFont" text="#DAC_Party_Context_Invite"/>
          </Panel>
          <Panel id="TrainingPlayButton" class="PlayButton" onactivate="DACPlayLaunchTraining()">
