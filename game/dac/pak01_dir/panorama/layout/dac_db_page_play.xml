              <Panel id="TrainingTypeButtonTutorial" class="GameModeOptionButton" onactivate="DACPlayTrainingSetMatchType(1)">
                <Label text="#DAC_MatchTypeName7"/>
              </Panel>
              <Label class="GameModeOptionsDesc TrainingDesc" text="#DAC_ReplayTheTutorial"/>
