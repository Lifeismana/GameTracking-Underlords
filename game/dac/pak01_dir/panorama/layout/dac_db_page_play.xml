            <Panel id="GCDuosBotDifficultyContainer" class="GameModeOptionsSettingsContainer">
              <Panel id="EasyGCDuosBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(5,1)">
                <Label text="#DAC_BotDifficultyName0"/>
              </Panel>

              <Panel id="MediumGCDuosBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(5,2)">
                <Label text="#DAC_BotDifficultyName1"/>
              </Panel>

              <Panel id="ChallengingGCDuosBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(5,3)">
