            <Panel id="GCTurboBotDifficultyContainer" class="GameModeOptionsSettingsContainer">
              <Panel id="EasyGCTurboBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(1,1)">
                <Label text="#DAC_BotDifficultyName0"/>
              </Panel>

              <Panel id="MediumGCTurboBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(1,2)">
                <Label text="#DAC_BotDifficultyName1"/>
              </Panel>

              <Panel id="ChallengingGCTurboBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(1,3)">
                <Label text="#DAC_BotDifficultyName2"/>
              </Panel>

              <Panel id="BattlepassGCTurboBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(1,4)">
                <Label text="#DAC_BotDifficultyName5"/>
