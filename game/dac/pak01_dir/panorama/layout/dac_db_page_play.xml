  <snippets>
	</snippets>

	<DACPlayPage hittest="false" class="DashboardPage">

		<Panel id="Background">
			<!-- <Image id="BGImage" src="s2r://panorama/images/dashboard/background/flattened-bg_jpg.vtex"/> -->
		</Panel>

    <Panel class="UnderlordSelectContainer" id="UnderlordSelector">
      <Panel id="SelectEmpty" onactivate="DACSelectDashboardButton('UnderlordsContainer'); DACShowUnderlordsPage(); PlaySoundEffect( 'DAC_UI.MainMenu_LoadoutActivate' )">
        <Label class="DistressedFont" hittest="false" html="true" text="#DAC_Underlord_NoneSelected" id="SelectEmpty" />
      </Panel>
      <Panel class="UnderlordSelectTopContainer">
        <Panel class="UnderlordSelectBackground"/>
        <Panel class="UnderlordSelectTop">
          <Button id="LeftUnderlordArrow" class="UnderlordArrow ArrowLeft" onactivate="DACPlayChangeUnderlord(-1)"/>
          <Panel class="UnderlordButton">
            <Label class="DistressedFont" html="true" text="{s:underlord_name}"/>
          </Panel>
          <Button id="RightUnderlordArrow" class="UnderlordArrow" onactivate="DACPlayChangeUnderlord(1)"/>
        </Panel>
      </Panel>
      <Panel class="UnderlordSelectMiddleContainer">
        <Panel id="ViewDetails" class="MiddleButton" onactivate="DACSelectDashboardButton('UnderlordsContainer'); DACShowUnderlordsPage(); PlaySoundEffect( 'DAC_UI.MainMenu_LoadoutActivate' )">
          <Label text="#DAC_Reward_Item_ViewDetails"/>
        </Panel>
        <Panel id="Select" class="MiddleButton" onactivate="DACPartyMemberToggleReady()">
          <Label text="#DAC_SelectUnderlord"/>
        </Panel>
        <Panel id="Deselect" class="MiddleButton" onactivate="DACPartyMemberToggleReady()">
          <Label text="#DAC_DeselectUnderlord"/>
        </Panel>
      </Panel>
      <Panel class="UnderlordSelectBottomContainer">
        <Label class="TalentsUnlockedNumbers HeadlineFont" text="#DAC_Reward_Item_TalentCount"/>
        <Label class="TalentsUnlocked" text="#DAC_Reward_Item_TalentUnlockedLabel"/>
        <Image class="InfoImage" onmouseover="DACShowTalentInfoTooltip()" onmouseout="DACHideTalentInfoTooltip()" onactivate="DACShowTalentInfoPopup()"/>
      </Panel>
    </Panel>

    <Panel id="ForegroundContainer">
			<Panel id="GameModeContainer">
        <Panel id="GameModeSelectionContainer">
          <Panel id="TurboGameModeButton" class="GameModeButton" onactivate="DACPlaySelectMode(0)">
            <Panel class="GameModeDescContainer">
              <Label text="#DAC_MatchTypeTurbo" class="GameModeButtonTitle HeadlineFont"/>
              <Label text="#DAC_MatchTypeDescTurbo" class="GameModeButtonDesc"/>
            </Panel>  
          </Panel>
          <Panel id="StandardGameModeButton" class="GameModeButton" onactivate="DACPlaySelectMode(1)">
            <Panel class="GameModeDescContainer">
              <Label text="#DAC_MatchTypeStandard" class="GameModeButtonTitle HeadlineFont"/>
              <Label text="#DAC_MatchTypeDescStandard" class="GameModeButtonDesc"/>
            </Panel>
          </Panel>
          <Panel id="DuosGameModeButton" class="GameModeButton" onactivate="DACPlaySelectMode(2)">
            <Panel class="GameModeDescContainer">
              <Label text="#DAC_MatchTypeName3" class="GameModeButtonTitle HeadlineFont"/>
              <Label text="#DAC_MatchTypeDescDuos" class="GameModeButtonDesc"/>
            </Panel>
          </Panel>
          <Panel id="TrainingGameModeButton" class="GameModeButton" onactivate="DACPlaySelectMode(3)">
             <Panel class="GameModeDescContainer">
              <Label text="#DAC_Training" class="GameModeButtonTitle HeadlineFont"/>
              <Label text="#DAC_MatchTypeDescTraining" class="GameModeButtonDesc"/>
            </Panel>
          </Panel>
        </Panel>

        <Panel id="GameModeOptionsContainer">
          <!-- *************************Turbo Options************************* -->
          <Panel id="TurboOptions" class="GameModeOptionsPanel">
            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="GameModeOptionsTitle HeadlineFont" text="#DAC_MatchTypeTurbo"/>
            </Panel>
            
            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="OptionHeader" text="#DAC_MatchOpponent"/>
              <Panel id="TurboMatchTypeButtonMultiplayer" class="GameModeOptionButton" onactivate="DACPlayTurboSetMatchType(0)">
                <Label text="#DAC_MatchTypeMultiplayer"/>
              </Panel>

              <Panel id="TurboMatchTypeButtonBots" class="GameModeOptionButton" onactivate="DACPlayTurboSetMatchType(3)">  <!--EDACMatchMode-->
                <Label text="#DAC_MatchTypeBotMatch"/>
              </Panel>
            </Panel>



            <Panel class="GameModeOptionsSettingsContainer">
              <Panel id="TurboSubSettingMultiplayer" class="GameModeOptionsSettingsSubContainer">
                 <Panel id="TurboMatchmakingTypeButtonCasual" class="GameModeOptionButton" onactivate="DACPlayTurboSetMatchmakingType(1)">  <!--EDACMatchMode-->
                  <Label text="#DAC_MatchTypeName0"/>
                </Panel>

              </Panel>

              <Panel id="TurboSubSettingBots" class="GameModeOptionsSettingsSubContainer">
                <Label class="OptionHeader" text="#DAC_BotDifficulty_Desc"/>
                <Panel id="LocalTurboBotDifficultyButton" class="BotDifficultyDropdown" onactivate="DACPlayDisplayBotDifficulty(0)">  <!--EPlayBotDifficultyType-->
                  <Label id="TurboBotDifficultyLabel" text="{s:local_turbo_bot_difficulty}"/>
                </Panel>
                <Panel id="GCTurboBotDifficultyButton" class="BotDifficultyDropdown" onactivate="DACPlayDisplayBotDifficulty(1)">  <!--EPlayBotDifficultyType-->
                  <Label id="TurboBotDifficultyLabel" text="{s:gc_turbo_bot_difficulty}"/>
                </Panel>
              </Panel>

              <Panel id="TurboSubSettingResume" class="GameModeOptionsSettingsSubContainer">
                <Panel id="TurboResumeBotGameButton" class="GameModeOptionButton" onactivate="DACPlaySetSaveGameAction(false)">
                  <Label text="#DAC_Play_ResumeBots"/>
                </Panel>
                <Panel id="TurboDeleteBotGameButton" class="GameModeOptionButton" onactivate="DACPlaySetSaveGameAction(true)">
                  <Label text="#DAC_Home_DeleteSave"/>
                </Panel>

                <Panel class="ResumeInfoContainer">
                  <Label class="PlayPageLabel ResumeLabel" text="#DAC_ResumeInfo_Desc"/>
                  <Label class="PlayPageLabel ResumeLabel" text="#DAC_ResumeInfo_Timestamp"/>
                </Panel>
              </Panel>
            </Panel>
          </Panel>

          <!-- *************************Turbo Bot Difficulty************************* -->
          <Panel id="TurboBotDifficulty" class="GameModeOptionsPanel">
            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="GameModeOptionsTitle HeadlineFont" text="#DAC_MatchTypeTurbo"/>
            </Panel>

            <Panel id="LocalTurboBotDifficultyContainer" class="GameModeOptionsSettingsContainer">
              <Label class="OptionHeader" text="#DAC_BotDifficulty_Desc"/>
