              <Panel id="MediumLocalStandardBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(2,1)">
                <Label text="#DAC_BotDifficultyName1"/>
              </Panel>

              <Panel id="ChallengingLocalStandardBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(2,2)">
                <Label text="#DAC_BotDifficultyName2"/>
              </Panel>

              <Panel id="HardcoreLocalStandardBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(2,3)">
                <Label text="#DAC_BotDifficultyName3"/>
              </Panel>

              <Panel id="BattlepassLocalStandardBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(2,4)">
                <Label text="#DAC_BotDifficultyName5"/>
              </Panel>
            </Panel>

            <Panel id="GCStandardBotDifficultyContainer" class="GameModeOptionsSettingsContainer">
              <Panel id="EasyGCStandardBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(3,1)">
                <Label text="#DAC_BotDifficultyName0"/>
              </Panel>

              <Panel id="MediumGCStandardBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(3,2)">
                <Label text="#DAC_BotDifficultyName1"/>
              </Panel>

              <Panel id="ChallengingGCStandardBotOptionButton" class="GameModeOptionButton" onactivate="DACPlaySetBotDifficulty(3,3)">
                <Label text="#DAC_BotDifficultyName2"/>
