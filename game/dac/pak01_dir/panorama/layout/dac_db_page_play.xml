          <!-- *************************Training Options************************* -->
          <Panel id="TrainingOptions" class="GameModeOptionsPanel">
            <Panel class="GameModeOptionsSettingsContainer">
              <Label class="GameModeOptionsTitle HeadlineFont" text="#DAC_Training"/>
            </Panel>

            <Panel class="GameModeOptionsSettingsContainer">
              <Panel id="TrainingTypeButtonFreestyle" class="GameModeOptionButton" onactivate="DACPlayTrainingSetMatchType(0)">
                <Label text="#DAC_MatchTypeName5"/>
