    <Panel class="DashboardHeader">
			<Panel id="BackButton" onactivate="DACSelectDashboardButton('PlayContainer'); DACShowHomePage(); PlaySoundEffect( 'DAC_UI.MainMenu_GameActivate' );" />
    </Panel>    
	</DACPlayPage>
