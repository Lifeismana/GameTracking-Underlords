            <Label id="DeleteGameLabel" class="BeginMatchLabel HeadlineFont" text="#DAC_Home_DeleteSave"/>
          </Panel>
          <Label id="DuosLockedLabel" class="LockedLabel" hittest="false" text="{s:duos_state}"/>
