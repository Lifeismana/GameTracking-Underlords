<root>
	<styles>
		<include src="s2r://panorama/styles/gamestyles.vcss_c" />
		<include src="s2r://panorama/styles/dac_global.vcss_c" />
		<include src="s2r://panorama/styles/dac_world_main.vcss_c" />
	</styles>

	<DACWorldMain>
		<Panel id="UnitCountDisplay" hittest="false">
			<Panel id="UnitCountPanel" hittest="false">
				<Panel id="UnitCountIconPanel">
					<Panel id="UnitCountIcon" />
				</Panel>
				<!-- DAC_InGame_UnitCount/DAC_InGame_UnitCount -->
				<Label id="UnitCountLabel_Finite" class="UnitCountLabel" html="true" text="#DAC_InGame_UnitCount">
					<Label class="UnitCountLabelShadow" html="true" text="#DAC_InGame_UnitCount" />
				</Label>
				<Label id="UnitCountLabel_Infinite" class="UnitCountLabel" html="true" text="#DAC_InGame_UnitCount_InfiniteLimit">
					<Label class="UnitCountLabelShadow" html="true" text="#DAC_InGame_UnitCount_InfiniteLimit" />
				</Label>
			</Panel>
		</Panel>
		<Panel id="DuosContainer" class="TopBottomFlow">
			<Label class="BoardTeammateLabel" text="{g:dac_player_slot_persona:b:duos_partner_player_slot}" html="true" />
			<Label class="BoardPrepLabel" text="#dac_board_team_prep" />
			<Label class="BoardPrepLabel WaitingLabel" text="#dac_board_waiting_for_teammate" />
		</Panel>
		<Label class="BoardSpectatingLabel" text="{g:dac_player_slot_persona:b:selected_player_slot}" html="true" />
	</DACWorldMain>

</root>