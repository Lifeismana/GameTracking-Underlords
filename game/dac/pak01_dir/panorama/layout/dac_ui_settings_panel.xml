            </Panel>
          </Panel>
        </Panel>
      </Panel>
    </snippet>

    <snippet name="AboutTabContent">
      <Panel class="SettingsSecondaryPanel">
        <Panel id="AboutTabContentContainer">

          <Panel id="AboutDacLogo" />

          <Label class="AboutVersion" text="#dac_ClientVersion" html="true" />
          <Label class="AboutVersion" text="#dac_GameplayVersion" html="true" />

          <Image id="AboutValveLogo" />

          <Button id="CreditsButton" class="ButtonBevel" onactivate="UIPopupButtonClicked( DACShowCreditsPage() )">
            <Label text="#dac_Credits" />
          </Button>

        </Panel>
      </Panel>
    </snippet>

    <snippet name="GameTabContent">
      <Panel class="SettingsSecondaryPanel">
        <Panel class="SettingsColumnContainer" id="GameSettings">
          <Panel id="GameSettingsColumn" class="SettingsDualColumn">
            <Panel id="SteamSettingsSection" class="SettingsSection">
              <Label text="#dac_settings_steam" class="SectionHeader" />
              <!-- <Panel class="SectionHeaderLine"/> -->
              <Panel id="SteamLogo" />
              <Button id="LogoutButton" class="ButtonBevel" onactivate="DACSteamLogout()">
                <Label text="#DAC_SteamLogOut"/>
              </Button>
            </Panel>

            <Panel id="GameOptionsSection" class="SettingsSection">
              <Label text="#dac_settings_options" class="SectionHeader" />
              <!-- <Panel class="SectionHeaderLine"/> -->
               <Panel class="TopBottomFlow">
                  <Label class="SubTitle" text="#dac_settings_ui_mode" />
                  <DACSettingsEnumDropDown id="UIModeConfig" convar="dac_ui_mode">
                    <Label text="#dac_settings_ui_config_mobile" id="mobile"  value="0" />
                    <Label text="#dac_settings_ui_config_pc" id="pc"   value="1" />
                  </DACSettingsEnumDropDown>
                </Panel>
              <DACSettingsCheckbox convar="dac_synergies_hide_partial_in_combat" text="#dac_settings_synergies_hide_partial" />
            </Panel>

            <Panel class="SettingsSection HotkeySettingsSection">
              <Label text="#dac_settings_hotkeys" class="SectionHeader" />
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_sell"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_sell_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_shop"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_shop_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_unitpurchase"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_unitpurchase_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_reroll"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_reroll_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_lock"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_lock_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_levelup"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_levelup_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_tobench"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_tobench_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_tabs"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_tabs_desc"/>
              </Panel>
            </Panel>
          </Panel>
          <Panel id="HotkeySettingsColumn" class="SettingsDualColumn">
            <Panel class="SettingsSection HotkeySettingsSection">
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_home"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_home_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_enemies"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_enemies_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_opponent_board"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_opponent_board_desc"/>
              </Panel>
              <Panel class="HotkeyContainer">
                <Label class="HotkeyKey" text="#dac_hotkey_away_board"/>
                <Label class="HotkeyDesc" text="#dac_hotkey_away_board_desc"/>
              </Panel>
            </Panel>
          </Panel>
        </Panel>
      </Panel>
    </snippet>

		<snippet name="SecretTabContent">
			<Panel class="SettingsSecondaryPanel">
				<Panel class="SecretOptionSection">
					<Label class="Header" text="#dac_settings_support_options" />
					
					<Button id="UploadSaveGameButton" class="ButtonBevel" onactivate="DACUploadSaveGameToSupport()">
						<Label text="#dac_support_upload_save_game" />
					</Button>

					<Button id="UploadConsoleLogButton" class="ButtonBevel" onactivate="DACUploadConsoleLogToSupport()">
						<Label text="#dac_support_upload_console_log" />
					</Button>
				</Panel>
				
				<Panel class="SecretOptionSection">
					<Label class="Header" text="#dac_settings_developer_options" />
					
					<DACSettingsCheckbox convar="dac_developer_mode" text="#dac_settings_dac_developer_mode" />
				</Panel>
			</Panel>
		</snippet>

	</snippets>

