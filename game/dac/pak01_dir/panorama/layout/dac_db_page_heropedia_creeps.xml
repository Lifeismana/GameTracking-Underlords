<root>
	<styles>
		<include src="s2r://panorama/styles/gamestyles.vcss_c" />
		<include src="s2r://panorama/styles/dac_db_page_heropedia_creeps.vcss_c" />
	</styles>

	<snippets>
	    <snippet name="RoundSnippet">  
		  <Panel class="RoundContainer">
		    <Panel id="RoundName">
	          <Panel id="PlayerProfileAvatar"/>
			  <Label class="RoundHeader" text="{s:round_name}"/>
	        </Panel>
			<Panel id="ItemList" class="CreepListContainer">
				<Image id="LootRoundImage" class="NeutralRoundImage" src="s2r://panorama/images/hud/neutral_round25_vs_psd.vtex"/>
				<Label class="NeutralRoundDescription" text="{s:round_description}" html="true"/>
			</Panel>
	      </Panel>
	    </snippet>
	</snippets>
	
	<DACHeropediaCreepsPage hittest="false">
		 <Panel id="SeasonIntro">
            <Label class="Header" text="#Creeps_tab"/>
            <Label class="SectionType" text="#Creeps_sub"/>
         </Panel>
    	 <Panel id="CreepList" class="HeropediaList"/>
	</DACHeropediaCreepsPage>
</root>
