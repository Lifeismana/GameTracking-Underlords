<root>
  <styles>
    <include src="s2r://panorama/styles/gamestyles.vcss_c" />
    <include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
    <include src="s2r://panorama/styles/popups/popup_generic.vcss_c" />
    <include src="s2r://panorama/styles/popups/popup_rankup.vcss_c" />
  </styles>

  <PopupRankUp>
    <Panel class="Contents PopupPanel">
			<Panel class="MainBody">
				<Label class="Title" text="#DAC_RankUp_Title" />
				<Label class="Subheading" text="#DAC_RankUp_Subheading" />

        <Panel id="RankContainer">
          <DACRankIcon id="RankIcon" iconstyle="standalone"/>
        </Panel>

      </Panel>
			<Panel class="PopupBottomContainer">
				<Button id="ContinueButton" class="PopupButton" onactivate="UIPopupButtonClicked()">
					<Label text="#UI_OK"/>
				</Button>
			</Panel>	
		</Panel>
  </PopupRankUp>
</root>
