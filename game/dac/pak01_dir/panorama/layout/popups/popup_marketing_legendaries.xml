<root>
	<styles>
		<include src="s2r://panorama/styles/gamestyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_generic.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_marketing_legendaries.vcss_c" />
	</styles>

	<Panel class="PopupPanel Hidden" popupbackground="blur">
		<Panel id="CloseButton" onactivate="PopupBlurBackgroundClicked()" />
		<Panel id="PopupImage" />
		<Panel id="TextArea">
			<Label id="PopupIntro" text="#Popup_Contraptions_HighRollers_Intro" />
			<Panel id="TitleArea">
				<Panel id="TitleLine1">
					<Label class="TextTitle First" html="true" text="#Popup_Contraptions_HighRollers_Title" />
					<Label class="And" text="#Popup_Contraptions_HighRollers_TitleAnd" />
				</Panel>
				<Label class="TextTitle Second" html="true" text="#Popup_Contraptions_HighRollers_Title2" />
			</Panel>

			<Label id="PopupDesc" text="#Popup_Contraptions_HighRollers_Desc" />

			<Label id="Button" text="#Popup_Contraptions_HighRollers_Btn" onactivate="UIPopupButtonClicked( DACShowPatchNotesPage() );" />
		</Panel>
	</Panel>
</root>