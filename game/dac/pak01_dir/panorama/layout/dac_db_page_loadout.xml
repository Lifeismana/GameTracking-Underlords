<root>
	<styles>
		<include src="s2r://panorama/styles/gamestyles.vcss_c" />
		<include src="s2r://panorama/styles/dac_db_page_loadout.vcss_c" />
	</styles>
	
	<DACLoadoutPage hittest="false" class="DashboardPage">
		<Panel class="Dimmer">
			<Panel id="TopRow">
				<Label class="PageTitleLabel" text="#DAC_Loadout" />
				<Label class="PageDescription" text="#DAC_Loadout_ComingSoon_Desc" />
			</Panel>
			<Panel id="BottomRow">
				<Panel class="LoadoutContainer Left">
					<Panel class="LoadoutBanners LoadoutDetail">
						<Label class="LoadoutLabel" text="#DAC_Loadout_Banners" />
					</Panel>
				</Panel>
				<Panel class="LoadoutContainer">
					<Panel class="LoadoutMaps LoadoutDetail">
						<Label class="LoadoutLabel" text="#DAC_Loadout_Maps" />
					</Panel>
				</Panel>
				<Panel class="LoadoutContainer Right">
					<Panel class="LoadoutFireworks LoadoutDetail">
						<Label class="LoadoutLabel" text="#DAC_Loadout_Fireworks" />
					</Panel>
					<Panel class="LoadoutTeleports LoadoutDetail">
						<Label class="LoadoutLabel" text="#DAC_Loadout_Teleports" />
					</Panel>
					<Panel class="LoadoutContainerBottom">
						<Panel class="LoadoutYos LoadoutDetail">
							<Label class="LoadoutLabel" text="#DAC_Loadout_Yos" />
						</Panel>
						<Panel class="LoadoutStreaks LoadoutDetail">
							<Label class="LoadoutLabel" text="#DAC_Loadout_Streaks" />
						</Panel>
					</Panel>
				</Panel>
			</Panel>
			<Label class="ComingSoonViolator" text="#DAC_ComingSoon" />
		</Panel>
	</DACLoadoutPage>
</root>
